<?xml version='1.0'?>

<robot name="ezrassor_arm"
    xmlns:xacro="http://www.ros.org/wiki/xacro">

    <!-- Including separate components of the robot -->
    <xacro:include filename="$(find ezrassor_sim_description)/urdf/paver_arm_kartik.xacro"/>

    <!-- Defining Main Body -->
    <!-- LINK 1-->
    <link name='link1'>
        <pose>0 0 0 0 0 0</pose>
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <inertia
                ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0"
            />
        </inertial>
        <collision name='collision'>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <geometry>
                <xacro:LINK1/>
            </geometry>
        </collision>
        <visual name='link1'>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <geometry>
                <xacro:LINK1/>
            </geometry>
        </visual>
    </link>
    <!-- LINK 2-->
    <link name='link2'>
        <pose>0 0 0 0 0 0</pose>
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <inertia
                ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0"
            />
        </inertial>
        <collision name='collision'>
            <origin xyz="0 0.08 0.135" rpy=" 0 0 0"/>
            <geometry>
                <xacro:LINK2/>
            </geometry>
        </collision>
        <visual name='link2'>
            <origin xyz="0 0.08 0.135" rpy=" 0 0 0"/>
            <geometry>
                <xacro:LINK2/>
            </geometry>
        </visual>
    </link>
    <!-- LINK 3-->
    <link name='link3'>
        <pose>0 0 0 0 0 0</pose>
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <inertia
                ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0"
            />
        </inertial>
        <collision name='collision'>
            <origin xyz="0.25 0.1 0" rpy=" 0 0 0"/>
            <geometry>
                <xacro:LINK3/>
            </geometry>
        </collision>
        <visual name='link3'>
            <origin xyz="0.25 0.1 0" rpy=" 0 0 0"/>
            <geometry>
                <xacro:LINK3/>
            </geometry>
        </visual>
    </link>
    <!-- LINK 4-->
    <link name='link4'>
        <pose>0 0 0 0 0 0</pose>
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <inertia
                ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0"
            />
        </inertial>
        <collision name='collision'>
            <origin xyz="0.23 0 0" rpy=" 0 0 0"/>
            <geometry>
                <xacro:LINK4/>
            </geometry>
        </collision>
        <visual name='link4'>
            <origin xyz="0.23 0 0" rpy=" 0 0 0"/>
            <geometry>
                <xacro:LINK4/>
            </geometry>
        </visual>
    </link>
    <!-- LINK 5-->
    <link name='link5'>
        <pose>0 0 0 0 0 0</pose>
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <inertia
                ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0"
            />
        </inertial>
        <collision name='collision'>
            <origin xyz="0 0.12 0" rpy=" 0 0 0"/>
            <geometry>
                <xacro:LINK5/>
            </geometry>
        </collision>
        <visual name='link5'>
            <origin xyz="0 0.12 0" rpy=" 0 0 0"/>
            <geometry>
               <xacro:LINK5/>
            </geometry>
        </visual>
    </link>
    <!-- LINK 6-->
    <link name='link6'>
        <pose>0 0 0 0 0 0</pose>
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <inertia
                ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0"
            />
        </inertial>
        <collision name='collision'>
            <origin xyz="0.096 0 -0.24" rpy=" 0 0 0"/>
            <geometry>
                <xacro:LINK6/>
            </geometry>
        </collision>
        <visual name='link6'>
            <origin xyz="0.096 0 -0.24" rpy=" 0 0 0"/>
            <geometry>
                 <xacro:LINK6/>
            </geometry>
        </visual>
    </link>
    <!-- CAMERA-->
    <link name='arm_camera'>
        <pose>0 0 0 0 0 0</pose>
        <inertial>
            <mass value="0.1"/>
            <origin xyz="0.0 0.0 0.0" rpy=" 0 0 0"/>
            <inertia
                ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0"
            />
        </inertial>
        <visual name='arm_camera'>
            <origin xyz="0.04 0.0 -0.04" rpy=" 0 0 0"/>
            <geometry>
                <xacro:CAMERA/>
            </geometry>
        </visual>
    </link>
    <!-- GRABBER1-->    
    <link name='grabber1'>
        <pose>0 0 0 0 0 0</pose>
        <inertial>
            <mass value="0.1"/>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <inertia
                ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0"
            />
        </inertial>
        <collision name='collision'>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <geometry>
                <xacro:GRABBER1/>
            </geometry>
            <contact_coefficients>
                <mu>1</mu>
            </contact_coefficients>
            <surface>
                <bounce>
                    <restitution_coefficient>0</restitution_coefficient>
                </bounce>
                <friction>
                    <ode>
                        <fdir1>0 0 0</fdir1>
                        <mu>1</mu>
                        <mu2>1</mu2>
                        <slip1>0</slip1>
                        <slip2>0</slip2>
                    </ode>
                </friction>
            </surface>
        </collision>
        <visual name='grabber1'>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <geometry>
                 <xacro:GRABBER1/>
            </geometry>
        </visual>
    </link>
    <!-- GRABBER2-->
    <link name='grabber2'>
        <pose>0 0 0 0 0 0</pose>
        <inertial>
            <mass value="0.1"/>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <inertia
                ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0"
            />
        </inertial>
        <collision name='collision'>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <geometry>
                 <xacro:GRABBER2/>
            </geometry>
            <contact_coefficients>
                <mu>1</mu>
            </contact_coefficients>
            <surface>
                <bounce>
                    <restitution_coefficient>0</restitution_coefficient>
                </bounce>
                <friction>
                    <ode>
                        <fdir1>0 0 0</fdir1>
                        <mu>1</mu>
                        <mu2>1</mu2>
                        <slip1>0</slip1>
                        <slip2>0</slip2>
                    </ode>
                </friction>
            </surface>
        </collision>
        <visual name='grabber2'>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <geometry>
                <xacro:GRABBER2/>
            </geometry>
        </visual>
    </link>
    <!-- PLATFORM-->    
    <link name='platform'>
        <pose>0 0 0 0 0 0</pose>
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <inertia
                ixx="0" ixy="0" ixz="0"
                iyy="0" iyz="0"
                izz="0"
            />
        </inertial>
        <collision name='collision'>
            <origin xyz="-0.5 0 0" rpy=" 0 0 0"/>
            <geometry>
                <box size="1 1 0.08"/>
            </geometry>
            <contact_coefficients>
                <mu>1</mu>
            </contact_coefficients>
            </collision>
            <visual name='platform'>
            <origin xyz="0.1 0.06 0.1" rpy=" 0 0 0"/>
            <geometry>
                <xacro:PLATFORM/>
            </geometry>
        </visual>
    </link>

    <!-- Linking all the robot parts -->
    <joint type="continuous" name="joint12">
        <origin xyz="0 -0.12 0.09" rpy="0 0 0"/>
        <child link="link2"/>
        <parent link="link1"/>
        <axis xyz="0 0 1" rpy="0 0 0"/>
        <joint_properties damping="0.0" friction="0.0"/>
    </joint>

    <joint type="continuous" name="joint23">
        <origin xyz="0 0.037 0.13" rpy="0 0 0"/>
        <child link="link3"/>
        <parent link="link2"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <joint_properties damping="0.0" friction="0.0"/>
    </joint>

    <joint type="continuous" name="joint34">
        <origin xyz="0.44 -0.037 0" rpy="0 0 0"/>
        <child link="link4"/>
        <parent link="link3"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <joint_properties damping="0.0" friction="0.0"/>
    </joint>

    <joint type="continuous" name="joint45">
        <origin xyz="0.42 0.0 0" rpy="0 0 0"/>
        <child link="link5"/>
        <parent link="link4"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <joint_properties damping="0.0" friction="0.0"/>
    </joint>

    <joint type="continuous" name="joint56">
        <origin xyz="0 0.12 0" rpy="0 0 0"/>
        <child link="link6"/>
        <parent link="link5"/>
        <axis xyz="0 0 1" rpy="0 0 0"/>
        <joint_properties damping="0.0" friction="0.0"/>
    </joint>

  
    <joint name="arm_camera_joint" type="fixed">
        <axis xyz="0 0 0" />
        <origin xyz="0 0.4 0" rpy="0 0.4 0"/>
        <parent link="link1"/>
        <child link="arm_camera"/>
    </joint>

    <joint name="platform_joint" type="fixed">
        <axis xyz="0 0 0" />
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="link1"/>
        <child link="platform"/>
    </joint>


    <joint type="prismatic" name="grabber_joint1">
        <origin xyz="0.1 -0.31 -0.24" rpy="0 0 0"/>
        <child link="grabber1"/>
        <parent link="link6"/>
        <axis xyz="1 1 0" rpy="0 0 0"/>
        <dynamics damping="1.0" friction="0.5"/>
        <limit lower="-0.02" upper="0.0224" effort="10" velocity="0.005"/>
    </joint>

    <joint type="prismatic" name="grabber_joint2">
        <origin xyz="0.1 0.31 -0.24" rpy="0 0 0"/>
        <child link="grabber2"/>
        <parent link="link6"/>
        <axis xyz="1 -1 0" rpy="0 0 0"/>
        <dynamics damping="1.0" friction="0.5"/>
        <limit lower="-0.02" upper="0.0225" effort="10" velocity="0.005"/>
    </joint>


    <!-- ROS2 Control Gazebo System -->

    
</robot>